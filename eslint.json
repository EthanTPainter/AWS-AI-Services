{
  "getter-return": true,
  "accessor-pairs": true,
  "array-callback-return": true,
  "class-methods-use-this": true,
  "curly": true,
  "default-case": true,
  "dot-notation": true,
  "max-classes-per-file": 1,
  "no-invalid-this": true,
  "no-magic-numbers": true,
  "no-multi-spaces": true,
  "no-return-await": true,
  "no-return-assign": true,
  "no-useless-catch": true,
  "radix": "always",
  "require-await": true,
  "yoda": "always",
  "init-declaration": ["error", "always"],
  "no-undef": true,
  "no-unused-vars": true,
  "no-use-before-define": true,
  "callback-return": false,
  "indent": ["error", 2],
  "prefer-arrow-callback": true
}