{
  "name": "aws-ai-services",
  "version": "1.0.0",
  "private": false,
  "description": "Tests the AWS AI Services",
  "author": "Ethan T. Painter",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "webpack-dev-server --mode development",
    "prod": "webpack --mode production",
    "clean": "rm -rf ./node_modules && rm -rf ./dist",
    "ci": "npm run clean && npm i && npm run lint && npm run build && npm run test",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src/**/*.js && eslint src/**/*.tsx",
    "lint:test": "eslint test/**/*.tsx",
    "test": "mocha -r ts-node/register test/**/*.spec.ts"
  },
  "bugs": {
    "url": "https://github.com/EthanTPainter/AWS-AI-Services/issues"
  },
  "homepage": "https://github.com/EthanTPainter/AWS-AI-Services#readme",
  "keywords": [
    "AWS",
    "AI",
    "Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanTPainter/AWS-AI-Services.git"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": true,
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "aws-sdk": "^2.566.0",
    "axios": "^0.19.0",
    "react": "0.0.0-experimental-b53ea6ca0",
    "react-dom": "0.0.0-experimental-b53ea6ca0",
    "react-router-dom": "^5.1.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.2",
    "@svgr/webpack": "^4.3.3",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-select": "^3.0.8",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.0",
    "prettier": "^1.19.1",
    "react-select": "^3.0.8",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
